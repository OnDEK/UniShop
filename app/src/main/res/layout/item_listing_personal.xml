<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="100dp">

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/bb_bottom_bar_top_shadow"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:id="@+id/listing_personal_item_thumbnail" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:textColor="#ff000000"
        android:inputType="text"
        android:text="Post title"
        android:ems="10"
        android:layout_alignParentTop="true"
        android:id="@+id/editText4"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/listing_personal_item_thumbnail"
        android:gravity="center_vertical"/>

    <TextView
        android:text="Biddding \nInfo"
        android:layout_width="wrap_content"
        android:layout_height="70dp"
        android:id="@+id/textView5"
        android:textAlignment="center"
        android:gravity="center"
        android:textColor="#ff000000"
        android:layout_below="@+id/editText4"
        android:layout_toEndOf="@+id/listing_personal_item_thumbnail" />

    <ImageView
        android:layout_width="23dp"
        android:layout_height="23dp"
        android:src="@drawable/ic_star_empty"
        android:layout_below="@+id/editText4"
        android:layout_toEndOf="@+id/textView5"
        android:id="@+id/imageView8" />

    <TextView
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal"
        android:ems="10"
        android:layout_below="@+id/editText4"
        android:layout_toEndOf="@+id/imageView8"
        android:id="@+id/editText5"
        android:textColor="#ff000000"
        android:layout_alignBottom="@+id/imageView8"
        android:gravity="center_vertical"/>

    <ImageView
        android:layout_width="23dp"
        android:layout_height="23dp"
        android:src="@drawable/ic_star_one"
        android:layout_below="@+id/imageView8"
        android:layout_toEndOf="@+id/textView5"
        android:id="@+id/imageView10" />

    <TextView
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal"
        android:ems="10"
        android:textColor="#ff000000"
        android:layout_toEndOf="@+id/imageView8"
        android:id="@+id/editText8"
        android:layout_alignBottom="@+id/imageView10"
        android:layout_below="@+id/imageView8"
        android:gravity="center_vertical"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="23dp"
        android:src="@drawable/ic_star_two"
        android:id="@+id/imageView12"
        android:layout_below="@+id/imageView10"
        android:layout_toEndOf="@+id/textView5"
        android:layout_alignBottom="@+id/textView5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        android:textColor="#ff000000"
        android:layout_below="@+id/editText8"
        android:layout_alignBottom="@+id/imageView12"
        android:layout_alignEnd="@+id/editText8"
        android:layout_toEndOf="@+id/imageView12"
        android:gravity="center_vertical"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_star_three"
        android:layout_below="@+id/editText4"
        android:layout_toEndOf="@+id/editText5"
        android:id="@+id/imageView13"
        android:layout_above="@+id/editText8" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_star_four"
        android:layout_below="@+id/editText5"
        android:layout_toEndOf="@+id/editText5"
        android:id="@+id/imageView14"
        android:layout_above="@+id/textView6" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_star_five"
        android:layout_below="@+id/editText8"
        android:layout_toEndOf="@+id/textView6"
        android:id="@+id/imageView15"
        android:layout_alignBottom="@+id/textView6" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editText4"
        android:id="@+id/textView7"
        android:textColor="#ff000000"
        android:layout_above="@+id/imageView14"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/imageView13"
        android:gravity="center_vertical"/>

    <TextView
        android:layout_width="wrap_content"
        android:textColor="#ff000000"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView15"
        android:id="@+id/textView8"
        android:layout_below="@+id/textView7"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/imageView14"
        android:gravity="center_vertical"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:layout_alignBottom="@+id/imageView15"
        android:id="@+id/textView9"
        android:textColor="#ff000000"
        android:layout_below="@+id/textView8"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/imageView15" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:id="@+id/button8"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:background="#00000000"/>

</RelativeLayout>